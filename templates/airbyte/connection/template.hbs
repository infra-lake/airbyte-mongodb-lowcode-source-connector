---
configurations:
  streams:
{{#each streams}}
  - name: {{ this.name }}
    syncMode: incremental_append
{{/each}}
schedule:
  scheduleType: manual
dataResidency: auto
namespaceDefinition: source
namespaceFormat: "${SOURCE_NAMESPACE}"
nonBreakingSchemaUpdatesBehavior: ignore
sourceId: {{ sourceId }}
destinationId: {{ destinationId }}
name: {{ name }}